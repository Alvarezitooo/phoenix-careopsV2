{
  "name": "phoenix-care",
  "version": "1.0.0",
  "description": "Plateforme d'assistance numÃ©rique pour parents d'enfants en situation de handicap",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "main": "server/dist/index.js",
  "scripts": {
    "build:server": "npm run -w server build",
    "build:client": "npm run -w client build",
    "build": "npm run build:server && npm run build:client",
    "dev": "npm run -w server dev & npm run -w client dev",
    "dev:server": "npm run -w server dev",
    "dev:client": "npm run -w client dev",
    "start": "npm run start:server",
    "start:server": "npm run -w server start",
    "start:client": "npm run -w client start",
    "docker:build": "docker build -t phoenix-care .",
    "docker:run": "docker run -p 3000:3000 phoenix-care",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf server/node_modules client/node_modules server/dist client/.next",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "lint": "cd server && npm run lint && cd ../client && npm run lint",
    "test": "cd server && npm test && cd ../client && npm test"
  },
  "keywords": [
    "phoenix-care",
    "handicap",
    "assistance",
    "parents",
    "social",
    "solidarity"
  ],
  "author": "PhoenixCare Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alvarezitooo/phoenix-careopsV2.git"
  },
  "bugs": {
    "url": "https://github.com/Alvarezitooo/phoenix-careopsV2/issues"
  },
  "homepage": "https://github.com/Alvarezitooo/phoenix-careopsV2#readme",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
